{
	"name": "electron-vue-starter",
	"version": "1.0.0",
	"description": "Electron/Vue.js Starter Project",
	"main": "init.js",
	"scripts": {
		"clean": "gulp clean",
		"dev": "cross-env NODE_ENV=development gulp serve",
		"dist": "gulp clean & cross-env NODE_ENV=production gulp build & build --windows --x64"
	},
	"author": {
		"name": "Luiz Guilherme Thess Cardoso",
		"email": "guithess@gmail.com"
	},
	"license": "MIT",
	"homepage": "https://github.com/guithess/electron-vue-starter#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/guithess/electron-vue-starter.git"
	},
	"keywords": [
		"vue.js",
		"start",
		"electron",
		"template",
		"boilerplate"
	],
	"dependencies": {
		"electron-updater": "^2.16.1",
		"revuest": "^1.1.4",
		"vue": "^2.5.3",
		"vue-resource": "^1.3.4",
		"vue-router": "^2.8.1"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-runtime": "^6.26.0",
		"browser-sync": "^2.18.13",
		"cross-env": "^5.1.1",
		"css-loader": "^0.28.7",
		"del": "^3.0.0",
		"electron": "^1.8.1",
		"electron-builder": "^19.45.4",
		"gulp": "^3.9.1",
		"gulp-rename": "^1.2.2",
		"gulp-sass": "^3.1.0",
		"gulp-sourcemaps": "^2.6.1",
		"gulp-watch": "^4.3.11",
		"gulp-zip": "^4.0.0",
		"npm-run-all": "^4.1.2",
		"run-sequence": "^2.2.0",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.18.2",
		"vue-devtools": "^3.1.9",
		"vue-html-loader": "^1.2.4",
		"vue-loader": "^13.4.0",
		"vue-template-compiler": "^2.5.3",
		"webpack": "^3.8.1",
		"webpack-stream": "^3.2.0"
	},
	"window": {
		"width": 800,
		"height": 600,
		"frame": true,
		"show": false
	},
	"build": {
		"appId": "electron-vue-starter",
		"artifactName": "${productName}-${version}.${ext}",
		"publish": [
			{
				"provider": "generic",
				"url": "http://127.0.0.1:8080"
			}
		]
	}
}
